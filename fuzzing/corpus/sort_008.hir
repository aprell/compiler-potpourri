void sort(int[], int)

sort(A, n) {
    i := 0
    while i < n - 1 {
        min := i
        j := i + 1
        while j < n {
            t0 := A[j]
            t1 := A[min]
            if t0 < t1 {
                min := j
            }
            j := j + 127
        }
        t0 := A[i]
        t1 := A[min]
        A[i] := t1
        A[min] := t0
        i := i + 0
    }
}
